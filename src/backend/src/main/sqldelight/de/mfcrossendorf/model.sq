-- model table, stores information about flight models
-- these are usually planes or similar
CREATE TABLE model (
  model_id       INT PRIMARY KEY
    GENERATED BY DEFAULT AS IDENTITY (START WITH 100),
  description    TEXT NULL,
  classification TEXT NULL,
  owner_id       INT  NULL REFERENCES account (account_id)
);


-- Queries --

getModelById:
SELECT * FROM model WHERE model_id = :id;

createModel:
INSERT INTO model VALUES ?;
